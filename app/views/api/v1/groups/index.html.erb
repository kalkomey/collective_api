<h1>Listing api_v1_groups</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_groups.each do |api_v1_group| %>
      <tr>
        <td><%= link_to 'Show', api_v1_group %></td>
        <td><%= link_to 'Edit', edit_api_v1_group_path(api_v1_group) %></td>
        <td><%= link_to 'Destroy', api_v1_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group', new_api_v1_group_path %>
